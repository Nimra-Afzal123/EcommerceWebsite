@model Feedback
@{
    ViewData["Title"] = "deletePermissionFeedback";
    Layout = "~/Views/Shared/_Dashboard.cshtml";
}
<h1 style="color:red">Are You Sure Want To Delete This Feedback?</h1>

<div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-8">

        <ul class="list-group my-3">
            <li class="list-group-item">Product Id: @Model.feedback_id</li>
			<li class="list-group-item">Product Name: @Model.user_name</li>
            <li class="list-group-item">Product Name: @Model.user_message</li>

        </ul>
        <a href="@Url.Action("deleteFeedback","Admin",new {id=Model.feedback_id})" class="btn btn-danger">Delete  Feedback</a>
        <a href="@Url.Action("fetchFeedback","Admin" )" class="btn btn-warning">Back To List</a>
    </div>
    <div class="col-md-2"></div>
</div>
